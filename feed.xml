---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Rob Tuley</title>
    <link>http://robtuley.github.io/</link>
    <atom:link href="http://robtuley.github.io/feed.xml" rel="self" type="application/rss+xml" />
    {% for post in site.posts limit:10 %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>http://robtuley.github.io/{{ post.url }}</link>
        <guid isPermaLink="true">http://robtuley.github.io/{{ post.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
